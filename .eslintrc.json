{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {"func-style": ["error", "declaration", { "allowArrowFunctions": false }],
  "spaced-comment": ["warn", "always"],
  "react/self-closing-comp": [
    "error",
    {
      "component": true,
      "html": false
    }
  ],
  "react/no-unescaped-entities": "off",
  "react/display-name": "off"
}
}